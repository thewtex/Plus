<!--
  This configuration is for volume reconstruction from US image slices recorded with
  Sonix Touch GPS machine, 14-5 linear probe.
  Image geometry: depth = 45mm, zoom = 100%.
-->
<Freehand>
  <SliceParameters SliceSpacing="1 1 1" SliceOrigin="0 0 0" NumScalarComponents="1" />
  
  <OutputParameters OutputSpacing="0.3 0.3 0.3" />
  
  <ClippingParameters ClipRectangle="148 41 492 442"/>
  <FanParameters FanAngles="0 0" FanOrigin="0 0" FanDepth="300"/>
  <RotationOptions ImageFlipped="Off" Rotation="Off" MaxRotationChange="90" Threshold1="0" Threshold2="255" ShiftX="0" ShiftY="0" />
  <ReconstructionOptions Interpolation="Linear" Optimization="On" Compounding="On"/>
  
  
  <SpatialCalibration CalibrationMatrix="  0.0000 0.1000  0  12.4790
                                           0.1000 0.0000  0  15.1253
                                           0.0000 0.0000  0   0.0213
                                           0      0       0   1.0000 "/>
  
</Freehand>
